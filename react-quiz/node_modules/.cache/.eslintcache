  const inc = function () {\n    dispatch({ type:'inc' });\n  };\n\n  const defineCount = function (e) {\n    dispatch({ type:'defCount', payload: Number(e.target.value) })\n  };\n\n  const defineStep = function (e) {\n    dispatch({ type:'defStep', payload: Number(e.target.value) })\n  };\n\n  const reset = function () {\n    dispatch({ type:'reset' })\n  };\n\n  return (\n    <div className=\"counter\">\n      <div>\n        <input\n          type=\"range\"\n          min=\"0\"\n          max=\"10\"\n          value={step}\n          onChange={defineStep}\n        />\n        <span>{step}</span>\n      </div>\n\n      <div>\n        <button onClick={dec}>-</button>\n        <input value={count} onChange={defineCount} />\n        <button onClick={inc}>+</button>\n      </div>\n\n      <p>{date.toDateString()}</p>\n\n      <div>\n        <button onClick={reset}>Reset</button>\n      </div>\n    </div>\n  );\n}\nexport default DateCounter;\n",["52"],"/Users/TT/WebstormProjects/react/learn-react/react-quiz/src/Header.js",[],[],"/Users/TT/WebstormProjects/react/learn-react/react-quiz/src/Main.js",[],[],"/Users/TT/WebstormProjects/react/learn-react/react-quiz/src/Error.js",[],[],"/Users/TT/WebstormProjects/react/learn-react/react-quiz/src/Loader.js",[],[],"/Users/TT/WebstormProjects/react/learn-react/react-quiz/src/StartScreen.js",[],[],"/Users/TT/WebstormProjects/react/learn-react/react-quiz/src/Question.js",[],[],{"ruleId":"53","replacedBy":"54"},{"ruleId":"55","severity":1,"message":"56","line":1,"column":21,"nodeType":"57","messageId":"58","endLine":1,"endColumn":29},{"ruleId":"53","replacedBy":"59"},"no-new-object",["60"],"no-unused-vars","'useState' is defined but never used.","Identifier","unusedVar",["60"],"no-object-constructor"]